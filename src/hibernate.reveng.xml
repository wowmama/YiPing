<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<type-mapping>
	<sql-type jdbc-type="INTEGER" hibernate-type="int"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="CHAR" hibernate-type="string"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="DATE" hibernate-type="date" length="10"
		not-null="false">
	</sql-type>
</type-mapping>
<table-filter match-name="customer100" match-catalog="yiping"></table-filter>
	<table-filter match-name="order100" match-catalog="yiping"></table-filter>
	<table-filter match-name="order101" match-catalog="yiping"></table-filter>
	<table-filter match-name="product100" match-catalog="yiping"></table-filter>
	<table-filter match-name="stock100" match-catalog="yiping"></table-filter>
	<table-filter match-name="stock101" match-catalog="yiping"></table-filter>
	<table-filter match-name="sys_status100" match-catalog="yiping"></table-filter>
</hibernate-reverse-engineering>